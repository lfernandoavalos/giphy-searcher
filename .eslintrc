{
  "extends": ["airbnb"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true,
  },
  "rules": {
    "max-len": "error",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "id-length": 2,
    "react/prop-types": [2],
    "indent": [1, 2],
    "no-console": 0,
    "no-unused-vars": "error",
    "strict": 0,
  },
}